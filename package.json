{
    "name": "4chanarchiver",
    "version": "0.1.0",
    "description": "Tool to archive 4chan written in NodeJS - Autism Edition",
    "main": "src/main.js",
    "scripts": {
        "lint": "eslint src/**",
        "test": "npm run lint",
        "build": "babel src -d build",
        "start": "npm run build && node build/main.js",
        "debug": "npm run build && node --inspect-brk=9229 build/main.js",
        "exec": "node build/main.js",
        "watch": "babel src -d build --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Wolvan/node_4ch.git"
    },
    "keywords": [
        "4chan",
        "archive",
        "archiving"
    ],
    "author": "Ctrl-S",
    "contributors": [
        "Wolvan"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Wolvan/node_4ch/issues"
    },
    "homepage": "https://github.com/Wolvan/node_4ch#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^5.2.0"
    },
    "dependencies": {
        "fs-extra": "^7.0.0",
        "limiter": "^1.1.3",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "sequelize": "^4.38.0",
        "sqlite": "^2.9.2",
        "tracer": "^0.9.1"
    }
}
